<table>
  <thead>
    <tr>
      <th>名称</th>
      <th>显示名称</th>
      <th colspan="2">操作</th>
    </tr>
  </thead>
  <tbody>
  <% @privileges.each do |pri| %>
      <tr>
        <td><%= pri.name %></td>
        <td><%= pri.displyname %></td>
        <td>
          <%= link_to '编辑',edit_privilege_path(pri) %>
          <%= link_to '删除', pri, method: :delete, data: { confirm: '确认删除?' } %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= link_to "新增",new_privilege_path%>
<%=render 'toolbar' %>
