<%= notice %>
<%= form_for @user, url: {action: "loginuser"}, html: {class: "m-t"} do |f| %>
    <div class="form-group">
      <%= f.text_field :username, autofocus: true, autocomplete: "email" ,placeholder:"用户名"%>
    </div>
    <div class="form-group">
      <%= f.password_field :pwd, autocomplete: "off" ,placeholder:"密码"%>
    </div>
    <%= f.submit "登 录" %>
    <%=link_to "注册",new_login_path%>
<% end %>